{
  "name": "react-playground",
  "version": "0.0.0",
  "repository": "https://github.com/hastebrot/react-playground",
  "author": "Benjamin Gudehus <hastebrot@gmail.com>",
  "private": true,
  "license": "EUPL",
  "scripts": {
    "start": "\"$npm_execpath\" run site:start",
    "build": "\"$npm_execpath\" run site:build",
    "test": "\"$npm_execpath\" run site:test",
    "bootstrap": "lerna bootstrap",
    "site:start": "lerna run start --stream --no-prefix --scope demo-react-site",
    "site:build": "lerna run build --stream --no-prefix --scope demo-react-site",
    "site:test": "lerna exec --scope demo-react-site -- \"$npm_execpath\" run test"
  },
  "devDependencies": {
    "lerna": "^3.11.0"
  }
}
